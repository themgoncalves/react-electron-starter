{
  "name": "react-electron-starter",
  "version": "1.0.0",
  "description": "React Webpack 3 Stater",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "electron .",
    "start:dev": "webpack --progress --profile --colors",
    "build": "webpack --config webpack.config.production.js --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": " webpack-dashboard -- webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "keywords": [
    "react",
    "webpack",
    "sass"
  ],
  "author": "Marcos Gon√ßalves",
  "license": "UNLICENSED",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0-alpha.6",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "popper.js": "^1.12.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "tether": "^1.4.0",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.8",
    "electron-reload": "^1.2.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.6.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "html-webpack-include-assets-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "loglevel": "^1.4.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.7.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest-setup-env.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss|less|css)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "expose-loader.*$": "<rootDir>/__mocks__/expose-loader.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/app/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/app/lib/**",
      "!src/scss/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "html"
    ]
  },
  "snyk": true
}
